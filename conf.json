{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "plugins/shout"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["src/controller", "src/services", "src/router"],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "encoding": "utf8",
        "destination": "./dist/",
        "readme": "README.md"
    }
}
